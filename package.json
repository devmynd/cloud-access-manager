{
  "name": "cloud-access-manager",
  "version": "0.0.0",
  "main": "index.js",
  "bin": {
    "cam": "lib/cli/program.js"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "prebuild": "flow",
    "core": "babel src/core -d lib/core",
    "api": "npm run core && babel src/web/api -d lib/web/api && node lib/web/api/app.js",
    "cli": "npm run prebuild && npm run core && babel src/cli -d lib/cli",
    "web": "npm run prebuild && webpack -d && npm run api",
    "launch": "open -a \"/Applications/Google Chrome.app\" lib/web/client/index.html",
    "watch": "webpack -d --watch",
    "test": "npm run prebuild  && npm run core && jest lib/",
    "standard": "standard --fix"
  },
  "repository": "git@github.com:devmynd/cloud-access-manager.git",
  "author": "Michael Evans <michael.evans@devmynd.com>",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.4.4",
    "commander": "^2.11.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "file-system": "^2.2.2",
    "font-awesome": "^4.7.0",
    "graphql": "^0.10.5",
    "heroku-client": "^3.0.2",
    "inquirer": "^3.1.1",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "terminal-kit": "^1.11.1",
    "webpack": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint-plugin-flowtype": "^2.34.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "fetch",
      "Headers",
      "describe",
      "test",
      "beforeEach",
      "expect"
    ]
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "flow",
      "react"
    ]
  }
}
