{
  "name": "cloud-access-manager",
  "version": "0.0.0",
  "main": "index.js",
  "bin": {
    "cam": "lib/cli.js"
  },
  "scripts": {
    "test": "standard && yarn build && jest",
    "build": "yarn flow && babel src/ -d lib/",
    "prepublish": "yarn run build",
    "fix": "standard --fix"
  },
  "repository": "git@github.com:devmynd/cloud-access-manager.git",
  "author": "Michael Evans <michael.evans@devmynd.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0",
    "file-system": "^2.2.2",
    "inquirer": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "flow-bin": "^0.49.1",
    "jest": "^20.0.4",
    "standard": "^10.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": "current"
        }
      }],
      "flow"
    ]
  }
}
